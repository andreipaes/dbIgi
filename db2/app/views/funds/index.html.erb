<p id="notice"><%= notice %></p>

<h1>Listing Funds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Shortname</th>
      <th>Isincode</th>
      <th>Cnpj</th>
      <th>Exclusive</th>
      <th>Nqpeergroupname</th>
      <th>Nipeergroupname</th>
      <th>Cvmclass</th>
      <th>Anbimaclass</th>
      <th>Adminfee</th>
      <th>Performancefeedescription</th>
      <th>Isigi</th>
      <th>Isigiexotic</th>
      <th>Hasrestriction</th>
      <th>Targetclientlist</th>
      <th>Minimuminvestmentamount</th>
      <th>Vehicletype</th>
      <th>Redemptionpaymentperiod</th>
      <th>Redemptionconversionperiod</th>
      <th>Totalshareholders</th>
       <th>Manager</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funds.each do |fund| %>
      <tr>
        <td><%= fund.name %></td>
        <td><%= fund.shortName %></td>
        <td><%= fund.isinCode %></td>
        <td><%= fund.cnpj %></td>
        <td><%= fund.exclusive %></td>
        <td><%= fund.nqPeerGroupName %></td>
        <td><%= fund.niPeerGroupName %></td>
        <td><%= fund.cvmClass %></td>
        <td><%= fund.anbimaClass %></td>
        <td><%= fund.adminFee %></td>
        <td><%= fund.performanceFeeDescription %></td>
        <td><%= fund.isIgi %></td>
        <td><%= fund.isIgiExotic %></td>
        <td><%= fund.hasRestriction %></td>
        <td><%= fund.targetClientList %></td>
        <td><%= fund.minimumInvestmentAmount %></td>
        <td><%= fund.vehicleType %></td>
        <td><%= fund.redemptionPaymentPeriod %></td>
        <td><%= fund.redemptionConversionPeriod %></td>
        <td><%= fund.totalShareHolders %></td>
        <td><%= fund.manager.name %></td>
        <td><%= link_to 'Show', fund %></td>
        <td><%= link_to 'Edit', edit_fund_path(fund) %></td>
        <td><%= link_to 'Destroy', fund, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

